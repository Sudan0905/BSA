bool bsa_free(bsa* b){
    if (b == NULL) {
        return false; // 如果BSA指针无效，不执行任何操作
    }
    // 释放每一行分配的内存
    for (int i = 0; i < BSA_ROWS; ++i) {
        if (b->row_allocated[i]) {
            free(b->rows[i]);  // 释放当前行
            b->rows[i] = NULL; // 将指针设置为NULL
        }
    }
    // 释放is_set分配的内存
    free(b->is_set);
    b->is_set = NULL; // 将指针设置为NULL
    // 释放BSA结构体本身
    free(b);
    return true;
}
